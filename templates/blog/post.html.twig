{% extends 'base.html.twig' %}

{% block title %} {{ post.title|upper }} {% endblock %}

{% block body %}

    <p>ID : {{ id }}</p>
    <h3>{{ post.title }}</h3>
    <p>{{ post.text }} </p>
    <p>{{ post.date|date('H:i:s') }}</p>
    <p>{{ 560|price }}</p>
    <p>Here is the current locale {{ locale }}</p>

    <p>{{ message }}</p>

    <p>ENV:{{ app.environment }}</p><br>
    <p>User:{{ app.user }}</p><br>
    <p>Request: {{ app.request }}</p><br>
    <p>Session Started : {{ app.session.started }}</p><br>
    <p>Debug: {{ app.debug }}</p>
{% endblock body %}